@model IEnumerable<MainCategory>

<h2> Main Category List </h2>
<a asp-action="Create" class="btn btn-success"> Create New</a>
<table class="table table-bordered"> 
     <tr>
         <td>Main Category ID</td>
         <td> Main Category Name</td>
     </tr>
    @foreach (var temp in Model)
    { 
      <tr>
          <td>@temp.MainCategoryID</td>
          <td>@temp.MainCategoryName</td>
          <td>
              <a asp-action="Edit" class="btn btn-success" asp-route-id="@temp.MainCategoryID">Edit</a>
              <a asp-action="Delete" class="btn btn-success" asp-route-id="@temp.MainCategoryID">Delete</a>
          </td>
      </tr>
    }
</table>